{
  "name": "dashli",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --env=jsdom --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "build": "rimraf dist && babel src --ignore **/stories,**/*.test.js,**/*.story.js --out-dir dist",
    "coveralls": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls",
    "start": "start-storybook -p 9009 -s public",
    "storybook": "start-storybook -p 9009 -s public",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build-storybook": "build-storybook -s public",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "dependencies": {
    "d3": "^5.7.0",
    "normalize.css": "^8.0.1",
    "point-in-polygon": "^1.0.1",
    "projections": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "timeago-react": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@storybook/addon-actions": "^4.0.7",
    "@storybook/addon-links": "^4.0.7",
    "@storybook/addons": "^4.0.7",
    "@storybook/react": "^4.0.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "kleur": "3",
    "node-sass-chokidar": "^1.3.4",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.1.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*.story.*",
      "!**/stories/**"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ]
  }
}
